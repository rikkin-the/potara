<h1>ユーザー登録</h1>

<div>
  <%= form_with(model: @user) do |f| %>
    <%= f.label :name %>
    <%= f.textfield :name %>

    <%= f.label :email %>
    <%= f.email_field :email %>

    <%= f.label :password %>
    <%= f.password_field :password %>

    <%= f.label :password_confirmation %>
    <%= f.password_field :password_confirmation %>

    <%= f. %>
    
