<h1>エントリー</h1>

<div id="location-result"></div>
<button type="button" id="get-position-btn">許可する</button>


<%= form_with model: current_user, id: "whole-form" do |f| %>
  <%= f.file_field :image, capture: 'camera' %>

  <%= f.label :comment, "つぶやき" %>
  <%= f.text_area :comment %>

  <%= f.submit "マッチを探す", id: "connect-link" %>
<% end %>